{
	"name": "@evangelos_bitsilis/v-treeview",
	"version": "0.0.2",
	"private": false,
	"library": "v-treeview",
	"license": "MIT",
	"description": "Component for Vue 3 with drag n drop and collapse functionality.",
	"author": "Evangelos Bitsilis <e.bitsilis@gmail.com>",
	"main": "dist/v-treeview.umd.js",
	"module": "dist/v-treeview.es.js",
	"style": "dist/style.css",
	"files": [
		"dist/*"
	],
	"keywords": [
		"Tree",
		"Vue 3",
		"Typescript",
		"Drag and drop",
		"Collapse"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/EvangelosBitsilis/V-Treeview.git"
	},
	"bugs": {
		"url": "https://github.com/EvangelosBitsilis/V-Treeview/issues"
	},
	"homepage": "https://github.com/EvangelosBitsilis/V-Treeview#readme",
	"scripts": {
		"dev": "vite",
		"build": "run-p type-check build-only",
		"preview": "vite preview --port 4173",
		"test:unit": "vitest --environment jsdom",
		"build-only": "vite build",
		"type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
		"lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
	},
	"dependencies": {
		"vue": "^3.2.36"
	},
	"devDependencies": {
		"@rushstack/eslint-patch": "^1.1.0",
		"@types/jsdom": "^16.2.14",
		"@types/node": "^16.11.36",
		"@vitejs/plugin-vue": "^2.3.3",
		"@vue/eslint-config-typescript": "^10.0.0",
		"@vue/test-utils": "^2.0.0",
		"@vue/tsconfig": "^0.1.3",
		"eslint": "^8.5.0",
		"eslint-plugin-vue": "^8.2.0",
		"jsdom": "^19.0.0",
		"npm-run-all": "^4.1.5",
		"typescript": "~4.7.2",
		"vite": "^2.9.9",
		"vitest": "^0.13.0",
		"vue-tsc": "^1.0.1"
	}
}
